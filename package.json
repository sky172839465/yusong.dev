{
  "name": "yusong.tw",
  "homepage": "https://yusong.tw",
  "private": true,
  "version": "0.213.0",
  "type": "module",
  "scripts": {
    "analyze": "analyze -c vite.config.js",
    "dev": "vite",
    "build": "run-s gen:routeData gen:imageData && vite build",
    "build:gh": "run-s build build:404",
    "build:404": "cp dist/index.html dist/404.html",
    "gen:data": "run-s gen:routeData gen:imageData gen:ogImageData",
    "gen:routeData": "mkdir -p src/data && mkdir -p public/data && node scripts/generateRouteData.js",
    "gen:imageData": "mkdir -p public/data && node scripts/generateImageData.js",
    "gen:ogImageData": "vite-node -c vite-node.config.js scripts/generateOgImageData.jsx",
    "icons": "pwa-assets-generator",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "patch-package",
    "preview": "vite preview",
    "prepare": "husky && run-s gen:routeData",
    "prettier": "prettier . -c",
    "prettier:fix": "prettier . --write"
  },
  "dependencies": {
    "@giscus/react": "^3.1.0",
    "@hookform/resolvers": "^4.1.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.5",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-hooks-library/core": "^0.6.2",
    "async-es": "^3.2.6",
    "axios": "^1.7.7",
    "axios-mock-adapter": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fn": "^0.0.2",
    "gray-matter": "^4.0.3",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.474.0",
    "motion": "^12.0.11",
    "query-string": "^9.1.0",
    "radash": "^12.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.4.1",
    "react-image": "^4.1.0",
    "react-router-dom": "^6.28.0",
    "shiki": "^1.24.0",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tinyglobby": "^0.2.10",
    "usehooks-ts": "^3.1.0",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.15",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.2",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "satori": "^0.12.1",
    "sharp": "^0.33.5",
    "simple-git": "^3.27.0",
    "tw-to-css": "^0.0.12",
    "vite": "^6.0.11",
    "vite-bundle-analyzer": "^0.16.2",
    "vite-node": "^3.0.4",
    "vite-plugin-markdown": "^2.2.0",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-sitemap": "^0.7.1",
    "workbox-window": "^7.3.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix"
    ],
    "**/!(package.lock).{json,md,yml,sh}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
